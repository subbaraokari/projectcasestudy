package org.ksr;

import java.util.Scanner;

public class BubbleSort {

	public static void main(String[] args) {
		int i,n;
		int[] a=new int[20];
		Scanner sc=new Scanner(System.in);
		System.out.println("Enter the number of elements");
		n=sc.nextInt();
		for(i=0;i<n;i++)
		{
			System.out.println("Enter element "+(i+1)+":");
			a[i]=sc.nextInt();
		}
	}

}
